{
    "compilerOptions": {
      "target": "es2015",
      "lib": ["es2015"],
      "types": ["reflect-metadata", "node"],
      "module": "commonjs",
      "moduleResolution": "node",
      "experimentalDecorators":true,
      "emitDecoratorMetadata": true,
      "sourceMap": true,
      "declaration": false,
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true,
      "outDir": "dist"
    }, 
    "typedocOptions": {
      "mode": "modules",
      "out": "docs"
  },
    "exclude": [
      "node_modules"
    ],
  
    
  }